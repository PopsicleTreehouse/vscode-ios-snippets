{
	"Objective-C Class Declaration": {
		"prefix": "@interface",
		"body": [
			"@interface ${ClassName} : ${Superclass}",
			"",
			"${methods/properties}",
			"",
			"@end"
		],
		"description": "Objective-C Class Declaration - iOS Snippets"
	},

	"Objective-C Class Extension": {
		"prefix": "@interface-extension",
		"body": [
			"@interface ${ClassName} ()",
			"",
			"${methods/properties}",
			"",
			"@end"
		],
		"description": "Objective-C Class Extension - iOS Snippets"
	},

	"Objective-C Category": {
		"prefix": "@interface-category",
		"body": [
			"@interface ${ClassName} (${CategoryName})",
			"",
			"${methods/properties}",
			"",
			"@end"
		],
		"description": "Objective-C Category - iOS Snippets"
	},

	"Objective-C Class Implementation": {
		"prefix": "@implementation",
		"body": [
			"@implementation ${ClassName}",
			"",
			"${methods}",
			"",
			"@end"
		],
		"description": "Objective-C Class Implementation - iOS Snippets"
	},

	"Objective-C Category Implementation": {
		"prefix": "@implementation-category",
		"body": [
			"@implementation ${ClassName} (${CategoryName})",
			"",
			"${methods}",
			"",
			"@end"
		],
		"description": "Objective-C Category Definition - iOS Snippets"
	},

	"Objective-C Protocol Definition": {
		"prefix": "@protocol",
		"body": [
			"@protocol ${ProtocolName} <NSObject>",
			"",
			"${methods}",
			"",
			"@end"
		],
		"description": "Objective-C Protocol Definition - iOS Snippets"
	}
}