{
    "Swift Class": {
		"prefix": "class",
		"body": [
		    "class ${name} {",
		    "\t${code}",
		    "}"
		],
		"description": "Swift Class - iOS Snippets"
	},

    "Swift Subclass": {
		"prefix": "class-subclass",
		"body": [
		    "class ${name}: ${superclass} {",
		    "\t${code}",
		    "}"
		],
		"description": "Swift Subclass - iOS Snippets"
	},

    "Swift Protocol": {
		"prefix": "protocol",
		"body": [
		    "protocol ${name} {",
		    "\t${requirements}",
		    "}"
		],
		"description": "Swift Protocol - iOS Snippets"
	},

   "Swift Extension": {
		"prefix": "extension",
		"body": [
		    "extension ${name} {",
		    "\t${code}",
		    "}"
		],
		"description": "Swift Extension - iOS Snippets"
	},

	"Swift Struct Declaration": {
		"prefix": "struct",
		"body": [
		    "struct ${name} {",
		    "\t${fields}",
		    "}"
		],
		"description": "Swift Struct Declaration - iOS Snippets"
	},

	"Swift Enumerated Type Declaration": {
		"prefix": "enum",
		"body": [
		    "enum ${name} {",
		    "\tcase $0",
		    "}"
		],
		"description": "Swift Enumerated Type Declaration - iOS Snippets"
	}
}