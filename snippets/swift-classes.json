{
    "Swift Class": {
		"prefix": "class",
		"body": [
		    "class ${name}",
		    "\t${code}",
		    "}"
		],
		"description": "Swift Class"
	},

    "Swift Subclass": {
		"prefix": "class-subclass",
		"body": [
		    "class ${name}: ${superclass}",
		    "\t${code}",
		    "}"
		],
		"description": "Swift Subclass"
	},

    "Swift Protocol": {
		"prefix": "protocol",
		"body": [
		    "protocol ${name}",
		    "\t${requirements}",
		    "}"
		],
		"description": "Swift Protocol"
	},

   "Swift Extension": {
		"prefix": "extension",
		"body": [
		    "extension ${name}",
		    "\t${code}",
		    "}"
		],
		"description": "Swift Extension"
	},

	"Swift Struct": {
		"prefix": "struct",
		"body": [
		    "struct ${name}",
		    "\t${fields}",
		    "}"
		],
		"description": "Swift Struct"
	}
}